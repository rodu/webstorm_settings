<templateSet group="user">
  <template name="func" value="function $functionName$() {&#10;  $body$&#10;}" description="function" toReformat="true" toShortenFQNames="true">
    <variable name="functionName" expression="&quot;functionName&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="body" expression="&quot;// body&quot;" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="excl" value="export class $ClassName$ {$END$}" description="Export Class" toReformat="true" toShortenFQNames="true">
    <variable name="ClassName" expression="" defaultValue="&quot;ClassName&quot;" alwaysStopAt="true" />
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="con" value="constructor() {&#10;    $END$&#10;}" description="Constructor (Rodu)" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_CLASS" value="true" />
      <option name="JS_CLASS" value="true" />
    </context>
  </template>
  <template name="imp" value="import { $Member$ } from '$module$';" description="Import (Rodu)" toReformat="true" toShortenFQNames="true">
    <variable name="Member" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="module" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="me" value="$methodName$($params$) {&#10;  $END$    &#10;}" description="Method (Rodu)" toReformat="true" toShortenFQNames="true">
    <variable name="methodName" expression="" defaultValue="&quot;methodName&quot;" alwaysStopAt="true" />
    <variable name="params" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_CLASS" value="true" />
      <option name="JS_CLASS" value="true" />
      <option name="TS_CLASS" value="true" />
    </context>
  </template>
  <template name="fun" value="($param$) =&gt; {&#10;  $END$&#10;}" description="(Rodu) Function ES6" toReformat="true" toShortenFQNames="true">
    <variable name="param" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_EXPRESSION" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="TS_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="funin" value="($param$) =&gt; $END$" description="(Rodu) Function Inline ES6" toReformat="true" toShortenFQNames="true">
    <variable name="param" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_EXPRESSION" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="TS_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="sudi" value="this.subscriptions.map(&#10;  (subscription: Subscription) =&gt; subscription.dispose()&#10;);" description="Subscriptions Dispose (Rodu)" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>